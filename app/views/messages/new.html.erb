<h1>New message</h1>
<%= turbo_frame_tag "new_message", target: "_top" do %>
   <%= form_with(model: [@message.room, @message]) do |form| %>
   <div>
      <%= form.label :content%>
      <%= form.text_field :content%>
      <%= form.submit "Send" %>
   </div>
   <% end %>
<% end %>
<%= link_to "back", @message.room %>